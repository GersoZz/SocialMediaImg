<div class="card mb-2">
  <div class="card-header bg-dark text-white">
    <h5> <i class="fas fa-fire"></i> Most Popular</h5>
  </div>
  <div class="card-body">
    <div class="row">
      {{#each sidebar.popular}}
        <div class="col-6 col-md-4 mb-3">
          <a href="/images/{{uniqueId}}">

            <img
              src="{{imageURL}}"
              class="w-100 h-100 image-thumbnail"
            />
          </a>
        </div>
      {{/each}}
    </div>
  </div>
</div>